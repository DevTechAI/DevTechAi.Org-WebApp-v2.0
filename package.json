{
  "name": "devtechai-webapp-v2",
  "version": "2.0.0",
  "description": "DevTechAI WebApp v2.0 - Comprehensive web application with AI, workflow, and cloud integrations",
  "main": "dist/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon --exec ts-node src/server.ts",
    "dev:client": "next dev",
    "start:webapp": "python3 server.py",
    "webapp": "./start.sh",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc -p tsconfig.server.json",
    "build:client": "next build",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:create": "prisma migrate dev --create-only",
    "db:migrate:rollback": "prisma migrate rollback",
    "db:seed": "ts-node prisma/seed.ts",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "docker:build": "docker build -t devtechai-webapp .",
    "docker:run": "docker run -p 3000:3000 devtechai-webapp",
    "docker:compose": "docker-compose up -d",
    "docker:compose:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "k8s:apply": "kubectl apply -f k8s/",
    "k8s:delete": "kubectl delete -f k8s/",
    "helm:install": "helm install devtechai-app ./helm-charts/app",
    "helm:upgrade": "helm upgrade devtechai-app ./helm-charts/app",
    "helm:uninstall": "helm uninstall devtechai-app",
    "terraform:init": "cd infrastructure/terraform && terraform init",
    "terraform:plan": "cd infrastructure/terraform && terraform plan",
    "terraform:apply": "cd infrastructure/terraform && terraform apply",
    "terraform:destroy": "cd infrastructure/terraform && terraform destroy",
    "deploy:staging": "npm run build && npm run k8s:apply",
    "deploy:production": "npm run build && npm run helm:upgrade",
    "monitor:logs": "kubectl logs -f deployment/devtechai-app",
    "monitor:metrics": "kubectl port-forward svc/prometheus 9090:9090",
    "monitor:dashboard": "kubectl port-forward svc/grafana 3000:3000",
    "security:audit": "npm audit",
    "security:scan": "snyk test",
    "security:fix": "npm audit fix",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx,json,md}",
    "format:check": "prettier --check src/**/*.{ts,tsx,js,jsx,json,md}",
    "clean": "rimraf dist .next node_modules/.cache",
    "prepare": "husky install",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^3.5.0",
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/auto-instrumentations-node": "^0.40.0",
    "@opentelemetry/exporter-jaeger": "^1.15.0",
    "@opentelemetry/sdk-node": "^0.45.0",
    "@prisma/client": "^5.7.0",
    "@sentry/node": "^7.85.0",
    "@sentry/profiling-node": "^1.0.0",
    "@supabase/supabase-js": "^2.38.0",
    "@anthropic-ai/sdk": "^0.9.0",
    "@google/generative-ai": "^0.2.0",
    "openai": "^4.20.0",
    "aws-sdk": "^2.1490.0",
    "@google-cloud/storage": "^7.7.0",
    "@google-cloud/pubsub": "^4.0.0",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-statsd": "^0.1.1",
    "pg": "^8.11.3",
    "prom-client": "^15.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.9.0",
    "@types/pg": "^8.10.7",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "eslint-config-next": "^14.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "lint-staged": "^15.1.0",
    "next": "^14.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "prisma": "^5.7.0",
    "rimraf": "^5.0.5",
    "snyk": "^1.1240.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "webapp",
    "ai",
    "workflow",
    "cloud",
    "typescript",
    "nextjs",
    "express",
    "kubernetes",
    "docker"
  ],
