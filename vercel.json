{
  "installCommand": "npm install --legacy-peer-deps",
  "buildCommand": "npm run build:client",
  "outputDirectory": "out",
  "framework": "nextjs",
  "routes": [
    {
      "src": "/index.html",
      "dest": "/index.html"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/services/(.*)",
      "dest": "/services/$1"
    },
    {
      "src": "/portfolio/(.*)",
      "dest": "/portfolio/$1"
    },
    {
      "src": "/solutions/(.*)",
      "dest": "/solutions/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

