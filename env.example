# Environment Configuration Template
# Copy this file to .env and fill in your actual values

# Application Configuration
NODE_ENV=development
APP_VERSION=2.0.0
PORT=3000
API_PORT=3001

# Database Configuration
DATABASE_PROVIDER=postgresql
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_NAME=devtechai
DATABASE_USER=postgres
DATABASE_PASSWORD=your_password_here
DATABASE_URL=postgresql://postgres:your_password_here@localhost:5432/devtechai

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Authentication Configuration
AUTH_PROVIDER=jwt
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRES_IN=15m
JWT_REFRESH_EXPIRES_IN=7d

# Auth0 Configuration (if using Auth0)
AUTH0_DOMAIN=your_domain.auth0.com
AUTH0_CLIENT_ID=your_client_id
AUTH0_CLIENT_SECRET=your_client_secret
AUTH0_AUDIENCE=your_api_identifier

# Firebase Configuration (if using Firebase)
FIREBASE_API_KEY=your_api_key
FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
FIREBASE_PROJECT_ID=your_project_id
FIREBASE_STORAGE_BUCKET=your_project.appspot.com
FIREBASE_MESSAGING_SENDER_ID=your_sender_id
FIREBASE_APP_ID=your_app_id

# Supabase Configuration (if using Supabase)
SUPABASE_URL=https://your_project.supabase.co
SUPABASE_ANON_KEY=your_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# AI Services Configuration
AI_PROVIDER=openai

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key
OPENAI_ORGANIZATION=your_organization_id

# Anthropic Configuration
ANTHROPIC_API_KEY=your_anthropic_api_key

# Google AI Configuration
GOOGLE_AI_API_KEY=your_google_ai_api_key

# Azure AI Configuration
AZURE_AI_ENDPOINT=https://your_resource.openai.azure.com
AZURE_AI_API_KEY=your_azure_api_key

# Cloud Services Configuration

# AWS Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1
AWS_S3_BUCKET=your_s3_bucket

# Google Cloud Configuration
GCP_PROJECT_ID=your_project_id
GCP_KEY_FILE=path/to/service-account.json
GCP_STORAGE_BUCKET=your_storage_bucket

# Oracle Cloud Configuration
OCI_TENANCY_ID=your_tenancy_id
OCI_USER_ID=your_user_id
OCI_FINGERPRINT=your_fingerprint
OCI_PRIVATE_KEY_PATH=path/to/private_key.pem
OCI_REGION=us-ashburn-1

# Azure Configuration
AZURE_SUBSCRIPTION_ID=your_subscription_id
AZURE_CLIENT_ID=your_client_id
AZURE_CLIENT_SECRET=your_client_secret
AZURE_TENANT_ID=your_tenant_id

# Workflow Services Configuration
WORKFLOW_PROVIDER=n8n

# N8N Configuration
N8N_BASE_URL=http://localhost:5678
N8N_API_KEY=your_n8n_api_key

# Zapier Configuration
ZAPIER_API_KEY=your_zapier_api_key

# Airtable Configuration
AIRTABLE_API_KEY=your_airtable_api_key
AIRTABLE_BASE_ID=your_base_id

# CI/CD Configuration
GITHUB_TOKEN=your_github_token
GITLAB_TOKEN=your_gitlab_token
JENKINS_URL=http://localhost:8080
JENKINS_USERNAME=your_username
JENKINS_API_TOKEN=your_api_token

# Monitoring Configuration
MONITORING_PROVIDER=datadog

# Datadog Configuration
DATADOG_API_KEY=your_datadog_api_key
DATADOG_APP_KEY=your_datadog_app_key
DATADOG_HOST=localhost
DATADOG_PORT=8125

# New Relic Configuration
NEW_RELIC_LICENSE_KEY=your_new_relic_license_key
NEW_RELIC_APP_NAME=DevTechAI WebApp

# Sentry Configuration
SENTRY_DSN=your_sentry_dsn
SENTRY_ORG=your_sentry_org
SENTRY_PROJECT=your_sentry_project

# Prometheus Configuration
PROMETHEUS_ENDPOINT=http://localhost:9090

# Grafana Configuration
GRAFANA_URL=http://localhost:3000
GRAFANA_USERNAME=admin
GRAFANA_PASSWORD=admin

# Jaeger Configuration
JAEGER_ENDPOINT=http://localhost:14268/api/traces

# ELK Stack Configuration
ELASTICSEARCH_URL=http://localhost:9200
LOGSTASH_URL=http://localhost:5044
KIBANA_URL=http://localhost:5601

# Security Configuration
CORS_ORIGIN=http://localhost:3000
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
CSRF_SECRET=your_csrf_secret

# File Upload Configuration
MAX_FILE_SIZE=10485760
UPLOAD_PATH=uploads/
ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,application/pdf

# Email Configuration
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_app_password
SMTP_FROM=noreply@devtechai.org

# SMS Configuration
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=your_twilio_phone_number

# Push Notifications
FCM_SERVER_KEY=your_fcm_server_key
APNS_KEY_ID=your_apns_key_id
APNS_TEAM_ID=your_apns_team_id
APNS_BUNDLE_ID=your_bundle_id

# Webhook Configuration
WEBHOOK_SECRET=your_webhook_secret
WEBHOOK_TIMEOUT=30000

# Cache Configuration
CACHE_TTL=3600
CACHE_MAX_SIZE=1000

# Logging Configuration
LOG_LEVEL=info
LOG_FORMAT=json
LOG_FILE=logs/app.log

# Feature Flags
FEATURE_FLAGS_ENABLED=true
FEATURE_FLAGS_PROVIDER=launchdarkly
LAUNCHDARKLY_SDK_KEY=your_launchdarkly_sdk_key

# Analytics Configuration
ANALYTICS_PROVIDER=google
GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
MIXPANEL_TOKEN=your_mixpanel_token

# Backup Configuration
BACKUP_ENABLED=true
BACKUP_SCHEDULE=0 2 * * *
BACKUP_RETENTION_DAYS=30
BACKUP_S3_BUCKET=your_backup_bucket

# Development Configuration
DEBUG=true
HOT_RELOAD=true
MOCK_SERVICES=false

# Testing Configuration
TEST_DATABASE_URL=postgresql://postgres:password@localhost:5432/devtechai_test
TEST_REDIS_URL=redis://localhost:6379/1

# Production Configuration (override in production)
# NODE_ENV=production
# DEBUG=false
# LOG_LEVEL=warn
# CORS_ORIGIN=https://devtechai.org
# DATABASE_URL=postgresql://user:password@prod-db:5432/devtechai
# REDIS_URL=redis://prod-redis:6379
