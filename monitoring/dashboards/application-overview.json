# Grafana Dashboard Configuration for DevTechAI WebApp v2.0

{
  "dashboard": {
    "id": null,
    "title": "DevTechAI WebApp v2.0 - Application Overview",
    "tags": ["devtechai", "webapp", "application"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Request Rate",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{method}} {{route}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec",
            "min": 0
          }
        ],
        "xAxis": {
          "show": true
        },
        "legend": {
          "show": true,
          "values": false,
          "current": false,
          "max": false,
          "min": false,
          "avg": false
        }
      },
      {
        "id": 2,
        "title": "Response Time",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "99th percentile",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Response Time (s)",
            "min": 0
          }
        ],
        "xAxis": {
          "show": true
        },
        "legend": {
          "show": true,
          "values": false,
          "current": false,
          "max": false,
          "min": false,
          "avg": false
        }
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "singlestat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 8
        },
        "targets": [
          {
            "expr": "rate(http_requests_total{status_code=~\"5..\"}[5m])",
            "legendFormat": "Error Rate",
            "refId": "A"
          }
        ],
        "valueName": "current",
        "format": "percent",
        "thresholds": "0.01,0.05",
        "colorBackground": true,
        "colorValue": false,
        "colors": ["#299c46", "#e24d42", "#d44a3a"]
      },
      {
        "id": 4,
        "title": "Active Users",
        "type": "singlestat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 8
        },
        "targets": [
          {
            "expr": "active_users_total",
            "legendFormat": "Active Users",
            "refId": "A"
          }
        ],
        "valueName": "current",
        "format": "short",
        "thresholds": "",
        "colorBackground": false,
        "colorValue": true,
        "colors": ["#299c46"]
      },
      {
        "id": 5,
        "title": "Database Connections",
        "type": "singlestat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 8
        },
        "targets": [
          {
            "expr": "database_connections_active",
            "legendFormat": "Active Connections",
            "refId": "A"
          }
        ],
        "valueName": "current",
        "format": "short",
        "thresholds": "50,80",
        "colorBackground": true,
        "colorValue": false,
        "colors": ["#299c46", "#e24d42", "#d44a3a"]
      },
      {
        "id": 6,
        "title": "Memory Usage",
        "type": "singlestat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 8
        },
        "targets": [
          {
            "expr": "process_resident_memory_bytes / 1024 / 1024",
            "legendFormat": "Memory Usage (MB)",
            "refId": "A"
          }
        ],
        "valueName": "current",
        "format": "short",
        "thresholds": "512,1024",
        "colorBackground": true,
        "colorValue": false,
        "colors": ["#299c46", "#e24d42", "#d44a3a"]
      },
      {
        "id": 7,
        "title": "CPU Usage",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 12
        },
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total[5m]) * 100",
            "legendFormat": "CPU Usage %",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "CPU %",
            "min": 0,
            "max": 100
          }
        ],
        "xAxis": {
          "show": true
        },
        "legend": {
          "show": true,
          "values": false,
          "current": false,
          "max": false,
          "min": false,
          "avg": false
        }
      },
      {
        "id": 8,
        "title": "Memory Usage Over Time",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 12
        },
        "targets": [
          {
            "expr": "process_resident_memory_bytes / 1024 / 1024",
            "legendFormat": "Memory Usage (MB)",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Memory (MB)",
            "min": 0
          }
        ],
        "xAxis": {
          "show": true
        },
        "legend": {
          "show": true,
          "values": false,
          "current": false,
          "max": false,
          "min": false,
          "avg": false
        }
      },
      {
        "id": 9,
        "title": "Top Routes by Request Count",
        "type": "table",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 20
        },
        "targets": [
          {
            "expr": "topk(10, sum(rate(http_requests_total[5m])) by (route))",
            "format": "table",
            "refId": "A"
          }
        ],
        "columns": [
          {
            "text": "Route",
            "value": "route"
          },
          {
            "text": "Requests/sec",
            "value": "Value"
          }
        ],
        "sort": {
          "col": 1,
          "desc": true
        }
      },
      {
        "id": 10,
        "title": "Error Status Codes",
        "type": "piechart",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 20
        },
        "targets": [
          {
            "expr": "sum(rate(http_requests_total[5m])) by (status_code)",
            "legendFormat": "{{status_code}}",
            "refId": "A"
          }
        ],
        "legend": {
          "show": true,
          "values": true,
          "current": false,
          "max": false,
          "min": false,
          "avg": false
        }
      },
      {
        "id": 11,
        "title": "Database Query Performance",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 28
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(database_query_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(database_query_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(database_query_duration_seconds_bucket[5m]))",
            "legendFormat": "99th percentile",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Query Duration (s)",
            "min": 0
          }
        ],
        "xAxis": {
          "show": true
        },
        "legend": {
          "show": true,
          "values": false,
          "current": false,
          "max": false,
          "min": false,
          "avg": false
        }
      },
      {
        "id": 12,
        "title": "AI Service Usage",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 36
        },
        "targets": [
          {
            "expr": "rate(ai_requests_total[5m])",
            "legendFormat": "{{service}} {{model}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec",
            "min": 0
          }
        ],
        "xAxis": {
          "show": true
        },
        "legend": {
          "show": true,
          "values": false,
          "current": false,
          "max": false,
          "min": false,
          "avg": false
        }
      },
      {
        "id": 13,
        "title": "Workflow Executions",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 36
        },
        "targets": [
          {
            "expr": "rate(workflow_executions_total[5m])",
            "legendFormat": "{{workflow_name}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Executions/sec",
            "min": 0
          }
        ],
        "xAxis": {
          "show": true
        },
        "legend": {
          "show": true,
          "values": false,
          "current": false,
          "max": false,
          "min": false,
          "avg": false
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "environment",
          "type": "query",
          "query": "label_values(http_requests_total, environment)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        },
        {
          "name": "instance",
          "type": "query",
          "query": "label_values(http_requests_total, instance)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "expr": "increase(deployment_events_total[1m]) > 0",
          "iconColor": "blue",
          "enable": true,
          "showIn": 0
        },
        {
          "name": "Alerts",
          "datasource": "Prometheus",
          "expr": "ALERTS{alertstate=\"firing\"}",
          "iconColor": "red",
          "enable": true,
          "showIn": 0
        }
      ]
    }
  }
}
